{
  "apiVersion": "apps/v1",
  "kind": "Deployment",
  "metadata": {
    "labels": {
      "test": "MyExampleApp"
    },
    "name": "terraform-example"
  },
  "spec": {
    "replicas": 1,
    "selector": {
      "matchLabels": {
        "test": "MyExampleApp"
      }
    },
    "template": {
      "metadata": {
        "labels": {
          "test": "MyExampleApp"
        }
      },
      "spec": {
        "containers": [
          {
            "image": "277341187485.dkr.ecr.eu-central-1.amazonaws.com/sample-app:latest",
            "name": "example",
            "ports": [
              {
                "containerPort": 80
              }
            ]
          }
        ]
      }
    }
  }
}