{
  "region": "eu-central-1",
  "aws_profile": "test",
  "vpc": {
    "cidr": "172.16.0.0/16",
    "private_subnets": [
      "172.16.1.0/24",
      "172.16.2.0/24",
      "172.16.3.0/24"
    ],
    "public_subnets": [
      "172.16.4.0/24",
      "172.16.5.0/24",
      "172.16.6.0/24"
    ],
    "enable_nat_gateway": true,
    "single_nat_gateway": true,
    "enable_dns_hostnames": true
  },
  "cluster": {
    "enabled": true,
    "name_prefix": "test-cluster",
    "tags": {
      "Environment": "test",
      "GithubRepo": "terraform-aws-eks"
    },
    "update_kube_config": true,
    "node_group": {
      "defaults": {
        "ami_type": "AL2_x86_64",
        "disk_size": 20
      },
      "members": {
        "ng1": {
          "desired_capacity": 1,
          "max_capacity": 3,
          "min_capacity": 1,
          "instance_type": "t3.small",
          "k8s_labels": {
            "Environment": "test",
            "GithubRepo": "terraform-aws-eks",
            "GithubOrg": "terraform-aws-modules"
          },
          "additional_tags": {
            "role": "worker"
          }
        }
      }
    },
    "deployments" : {
      "metrics_server" : {
        "enabled" : true
      },
      "kubernetes_dashboard" : {
        "enabled" : true,
        "csrf" : "myDashboardCsrf9876e"
      }
    }
  }
}